<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ARS_FE</title>


    <link rel="stylesheet" href="~/css/open-iconic-bootstrap.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true">

    <link rel="stylesheet" href="~/css/owl.carousel.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true">

    <link rel="stylesheet" href="~/css/aos.css" asp-append-version="true">

    <link rel="stylesheet" href="~/css/ionicons.min.css" asp-append-version="true">

    <link rel="stylesheet" href="~/css/bootstrap-datepicker.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/jquery.timepicker.css" asp-append-version="true">


    <link rel="stylesheet" href="~/css/flaticon.css">
    <link rel="stylesheet" href="~/css/icomoon.css">
    <link rel="stylesheet" href="~/css/style.css">

</head>
<body>
    <head>
        <style>
            /* Ensures header has a solid black background */
            #ftco-navbar {
                background-color: bisque !important; /* Black color */
                opacity: 1 !important; /* Remove any transparency */
            }
        </style>
    </head>
    <body>
        <header>
            <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
                <div class="container">
                    <a class="navbar-brand" href="index.html">Vacation<span>Travel Agency</span></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="oi oi-menu"></span> Menu
                    </button>

                    <div class="collapse navbar-collapse" id="ftco-nav">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item"><a class="nav-link" asp-page="/Staff/AirlinesManagement/Index">Airline</a></li>
                            <li class="nav-item"><a class="nav-link" asp-page="/Staff/AirportManagement/Index">Airport</a></li>
                            <li class="nav-item"><a class="nav-link" asp-page="/Staff/FlightClassManagement/Index">FlightClass</a></li>
                            <li class="nav-item"><a class="nav-link" asp-page="/Staff/AirplaneManagement/Index">Airplane</a></li>
                            <li class="nav-item"><a class="nav-link" asp-page="/UserPage/RankManagement/Index">Rank</a></li>
                            <li class="nav-item"><a class="nav-link" asp-page="/Staff/FlightManagement/Index">Flight</a></li>
                            
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
    </body>

        <!-- END nav -->
    </header>

 

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


  
   

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>